    <source class="responsive-img-srcset" media="(max-width: 1400px)" srcset="/assets/img/dissapointment-1400.webp" />
    

    <!-- Fallback to the original file -->
    <img src="/assets/img/dissapointment.jpeg" class="img-fluid rounded z-depth-1" width="300" height="300" data-zoomable="" onerror="this.onerror=null; $('.responsive-img-srcset').remove();" />

